{
  "name": "xstate-actor",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "main": "src/index.tsx",
  "scripts": {
    "database:esql:start": "docker-compose up",
    "database:esql:stop": "docker-compose down",
    "database:esql:show-config": "electric-sql show-config",
    "database:esql:generate": "drizzle-kit generate:sqlite --config=./database/drizzle.config.ts",
    "database:esql:proxy-tunnel": "electric-sql proxy-tunnel --service http://localhost:5133 --local-port 65431",
    "database:esql:migrate": "drizzle-kit generate:pg --config=./database/drizzle.config.ts",
    "database:esql:push": "drizzle-kit push:pg --config=./database/drizzle.config.ts",
    "database:esql:interactive": "drizzle-kit studio --config=./database/drizzle.config.ts",
    "database:esql:drop": "drizzle-kit drop --config=./database/drizzle.config.ts",
    "database:esql:check": "drizzle-kit check:pg --config=./database/drizzle.config.ts",
    "database:migrate": "vite-node ./database/migrate.ts",
    "lint": "biome check --apply --no-errors-on-unmatched .",
    "web": "vite",
    "web:build": "tsc && vite build",
    "web:esql:generate": "electric-sql generate --service http://localhost:5133 --proxy postgresql://postgres:password_1234@localhost:65432/database  --module-resolution bundler --debug true",
    "web:esql:watch": "yarn web:esql:generate --watch",
    "web:preview": "vite preview"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "@journeyapps/powersync-react": "^1.0.5",
    "@journeyapps/powersync-sdk-web": "^0.2.3",
    "@journeyapps/wa-sqlite": "^0.1.1",
    "@powersync/kysely-driver": "^0.1.1",
    "@statelyai/inspect": "^0.2.5",
    "@xstate/react": "^4.1.0",
    "call-bind": "^1.0.7",
    "classnames": "^2.3.2",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.29.4",
    "electric-sql": "^0.9.3",
    "kysely": "^0.27.2",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tooltip": "^5.26.0",
    "tarant": "github:guillempuche/tarant#v4.0.0",
    "tinybase": "^4.6.10",
    "ts-results": "^3.3.0",
    "uuid": "^9.0.0",
    "xstate": "^5.9.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.0",
    "@databases/pg": "^5.5.0",
    "@databases/pg-migrations": "^5.0.3",
    "@types/call-bind": "^1",
    "@types/node": "^20.11.20",
    "@types/pg": "^8.11.2",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.13",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.20.14",
    "env-cmd": "^10.1.0",
    "postcss": "^8.4.21",
    "rollup": "^4.12.1",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "vite": "^5.0.12",
    "vite-node": "^1.3.1",
    "vite-plugin-top-level-await": "^1.4.1",
    "vite-plugin-wasm": "^3.3.0"
  },
  "packageManager": "yarn@4.1.1"
}
